#!/usr/bin/perl

# download datasheet

my $HOST = "http://www.datasheetdir.com";
use samyweb;
die "usage: $0 [-s (starts) | -e (ends) | -c (contains, default)] <datasheet ...>\n" unless @ARGV;

my $searchmethod = 2;
if ($ARGV[0] eq '-s')
{
	shift @ARGV;
	$searchmethod = 1;
}
elsif ($ARGV[0] eq '-e')
{
	shift @ARGV;
	$searchmethod = 3;
}
elsif ($ARGV[0] eq '-c')
{
	shift @ARGV;
	$searchmethod = 2;
}

my $s = samyweb->new;

foreach my $ic (@ARGV)
{
	my $html = $s->get({ url => "$HOST/search.php", form => { part => $ic, searchmethod => $method, search => 'Search' }});
	my $pdf = 1;
	my @ics;

	#<td class="td1"> <a href='SN8P2501BP+Microcontrollers-8bit'>SN8P2501BP</a>  <br/><a href='SN8P2501BS+Microcontrollers-8bit'>SN8P2501BS</a>  <br/><a href='SN8P2501BX+Microcontrollers-8bit'>SN8P2501BX</a>  <br/><a href='SN8P2501BPDG+Microcontrollers-8bit'>SN8P2501BPDG</a>  <br/><a href='SN8P2501BSB+Microcontrollers-8bit'>SN8P2501BSB</a>  <br/>  <br/>   </td>                                                    


	print $html;
	while ($html =~ s/href='(.*?)'>(.*?)<\/a>\s*(?:&nbsp;)*\s*By (.*?)<\/li>\s*<li.*?>(.*?)<//mi)
	{
		my ($url, $name, $by, $desc) = ($1, $2, $3, $4);
		$desc =~ s/^\s*//;
		$name =~ s/^\s*//;
		$by   =~ s/^\s*//;
		$desc =~ s/\s*$//;
		$name =~ s/\s*$//;
		$by   =~ s/\s*$//;

		push @ics, [$url, $name, $by, $desc];
		print "$pdf: $name ($desc) $by\n";
		$pdf++;
	}

	my $i;
	while ($i !~ /^\d+$/)
	{
		print "> ";
		chomp($i = <STDIN>);
		$i =~ s/\s*//;
	}
	dl($ics[$i+1]);
}


sub dl
{
	my ($url, $name, $by, $desc) = @{$_[0]};

	$url =~ /(.*?)\+/;
	print $s->get({ url => "$HOST/go-$1.pdf", referer => "$HOST/$1+download" });
}

